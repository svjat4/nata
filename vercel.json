{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "sh setup.sh && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "main.py"}
    ]
}
